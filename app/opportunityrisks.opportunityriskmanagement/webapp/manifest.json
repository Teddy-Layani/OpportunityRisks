{
  "_version": "1.60.0",
  "sap.app": {
    "id": "opportunityrisks.opportunityriskmanagement",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.18.0",
      "toolsId": "9975c7b3-6601-47f0-bffa-7fa672fe359b"
    },
    "dataSources": {
      "mainService": {
        "uri": "/opportunity-risks/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "opportunityrisksopportunityris-display": {
          "semanticObject": "opportunityrisksopportunityris",
          "action": "display",
          "title": "{{opportunityrisksopportunityris-display.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.13",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "opportunityrisks.opportunityriskmanagement.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "OpportunityList",
          "target": "OpportunityList"
        },
        {
          "pattern": "Opportunity({key}):?query:",
          "name": "OpportunityObjectPage",
          "target": "OpportunityObjectPage"
        },
        {
          "pattern": "Opportunity({key})/risks({key2}):?query:",
          "name": "RiskObjectPage",
          "target": "RiskObjectPage"
        },
        {
          "pattern": "Risk({key}):?query:",
          "name": "RiskDirectAccess",
          "target": "RiskObjectPage"
        }
      ],
      "targets": {
        "OpportunityList": {
          "type": "Component",
          "id": "OpportunityList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Opportunity",
              "variantManagement": "Page",
              "navigation": {
                "Opportunity": {
                  "detail": {
                    "route": "OpportunityObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "OpportunityObjectPage": {
          "type": "Component",
          "id": "OpportunityObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Opportunity",
              "navigation": {
                "risks": {
                  "detail": {
                    "route": "RiskObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "risks/@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "enableExport": true
                  }
                }
              }
            }
          }
        },
        "RiskObjectPage": {
          "type": "Component",
          "id": "RiskObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Opportunity/risks"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}